{
    "name": "catreviews",
    "displayName": "Cat Reviews",
    "description": "Cats review your code!",
    "version": "1.2.0",
    "publisher": "okanaslan",
    "icon": "assets/many_error_cat.gif",
    "homepage": "https://github.com/okanaslan/CatFace",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/okanaslan/CatFace"
    },
    "engines": {
        "vscode": "^1.89.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [],
    "main": "./out/extension.js",
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "custom-activitybar",
                    "title": "Cat Reviews",
                    "icon": "asset/many_error_cat.png"
                }
            ]
        },
        "views": {
            "custom-activitybar": [
                {
                    "type": "webview",
                    "id": "vscodeSidebar.openview",
                    "name": "View",
                    "contextualTitle": "View"
                }
            ]
        },
        "commands": [
            {
                "command": "vscodeSidebar.openview",
                "title": "Sidebar View"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "build": "tsc",
        "compile": "npm run build",
        "lint": "eslint src --ext ts",
        "format": "prettier --write ."
    },
    "devDependencies": {
        "@types/node": "20.x",
        "@types/vscode": "^1.89.0",
        "@typescript-eslint/eslint-plugin": "^7.8.0",
        "@typescript-eslint/parser": "^7.8.0",
        "eslint": "^8.56.0",
        "prettier": "^3.2.5",
        "typescript": "^5.4.5"
    }
}
